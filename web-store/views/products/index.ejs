<h2>Search Products</h2>
<form action="/products" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOptions.name %> ">
    <button type="submit">Search</button>
</form>
<br>
<% for(let i = 0; i < products.length; i++) { %>
    <div>
       <%= products[i].productId %>
       <%= products[i].name %>
       <%= products[i].warranty %>
       <%= products[i].stock %>
       <%= products[i].price %>
   </div>
<% } %>